@model IEnumerable<ServicePetModel>

<div class="container-fluid py-5">
    <div class="container">
        <div class="border-start border-5 border-primary ps-5 mb-5" style="max-width: 600px;">
            <h6 class="text-primary text-uppercase">Dịch vụ</h6>
            <h1 class="display-5 text-uppercase mb-0">DỊCH VỤ CHĂM SÓC THÚ CƯNG TUYỆT VỜI CỦA CHÚNG TÔI</h1>
        </div>
        <div class="row g-5">
            @foreach (var item in Model)
            {
                <div class="col-md-6">
                    <div class="service-item bg-light d-flex p-4">
                        <i class="@item.icon"></i>
                        <div>
                            <h5 class="text-uppercase mb-3">@item.title</h5>
                            <p>@item.content</p>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
